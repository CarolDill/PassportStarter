<h1 class="mt-4">Dashboard</h1>
<p class="lead mb-3">Welcome <%= user.name %></p>
<br>
<p class="lead mb-3">Current Active Sessions:</p>
<br>
<% sessionsInfo.forEach(sessionInfo => { %>
  <p class="lead mb-1">SessionID: <%= sessionInfo %></p>
  <p class="lead mb-1">UserID: <%= sessions[sessionInfo].passport.user %> </p>
  <a href="/delete/<%= sessionInfo %>">Revoke</a>
  <br>
  <br>
  <br>
<% }) %>
<br>
<a href="/auth/logout" class="btn btn-secondary">Logout</a>